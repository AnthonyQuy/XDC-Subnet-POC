# XDC NetworkManager Frontend - Environment Variables Example
# Copy this file to .env in the frontend directory and add your values

# XDC Subnet RPC URL
# This is the RPC endpoint for your XDC subnet
REACT_APP_DEFAULT_RPC_URL=http://192.168.25.11:8545

# NetworkManager Contract Address
# The address of the deployed NetworkManager contract
REACT_APP_DEFAULT_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Gas Configuration (optional)
REACT_APP_GAS_LIMIT=500000
REACT_APP_GAS_PRICE=1000000000  # 1 gwei

# Network IDs
REACT_APP_SUBNET_CHAIN_ID=56316
